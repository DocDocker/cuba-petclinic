<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.cubaplatform.petclinic.web.pet.pettype.PetTypeEdit"
        datasource="petTypeDs"
        focusComponent="fieldGroup"
        messagesPack="com.cubaplatform.petclinic.web.pet.pettype">
    <dsContext>
        <datasource id="petTypeDs"
                    class="com.cubaplatform.petclinic.entity.pet.PetType"
                    view="_local"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="AUTO"
                width="600"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="petTypeDs">
            <column width="250px">
                <field property="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
